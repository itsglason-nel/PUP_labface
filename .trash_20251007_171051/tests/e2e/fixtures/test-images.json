    {
      "name": "no_face_detected",
      "description": "Test handling when no face is detected",
      "input_image": "no_face_image.jpg",
      "expected_result": "error",
      "expected_error": "No face detected"
    },
    {
      "name": "multiple_faces_detected",
      "description": "Test handling when multiple faces are detected",
      "input_image": "multiple_faces.jpg",
      "expected_result": "error",
      "expected_error": "Multiple faces detected"
    },
    {
      "name": "low_confidence_match",
      "description": "Test handling when face match confidence is low",
      "input_image": "student_face_2.jpg",
